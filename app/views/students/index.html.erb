<div ng-app="app">
    <div ng-controller="studentsCtrl" ng-init="setup()">
        <div class="container-fluid">
            <p>{{ message }}</p>
            <div class="row">
                <div class="col-sm-10">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="thumbnail" ng-repeat="student in students">
                                <div class="caption">
                                    <h3>Name:
                                        {{student.first_name}}
                                        {{student.last_name}}</h3>
                                    <p>Email:
                                        {{student.email}}</p>
                                    <p>Phone Number:
                                        {{student.phone_number}}</p>
                                    <p>Short Bio:
                                        {{student.short_bio}}</p>
                                    <p>Linkedin:
                                        {{student.linkedin_url}}</p>
                                    <p>Github:
                                        {{student.github_url}}</p>
                                    <p>Website:
                                        {{student.website_url}}</p><br>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="list-group">
                        <button type="button" class="list-group-item">Degree</button>
                        <button type="button" class="list-group-item">Degree</button>
                        <button type="button" class="list-group-item">Degree</button>
                        <button type="button" class="list-group-item">Degree</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
