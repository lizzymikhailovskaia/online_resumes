<div class="container-fluid">
  <div ng-app="app">
      <div ng-controller="studentsCtrl" ng-init="setup()">
          <div class="row">
              <div class="span10">
                  <div class="row">
                      <div class="col-md-12">
                        <h1>students information:
                        </h1>
                          <div class="thumbnail" ng-repeat="student in students">
                              <div class="caption">
                                  <h3>Name:
                                      {{student.first_name}}
                                      {{student.last_name}}</h3>
                                  <p>Email:
                                      {{student.email}}</p>
                                  <p>Phone Number:
                                      {{student.phone_number}}</p>
                                  <p>Short Bio:
                                      {{student.short_bio}}</p>
                                  <p>Linkedin:
                                      {{student.linkedin_url}}</p>
                                  <p>Github:
                                      {{student.github_url}}</p>
                                  <p>Website:
                                      {{student.website_url}}</p><br>
                              </div>
                          </div>
                      </div>

                  </div>
              </div>
              <div class="span2">
              </div>
          </div>
      </div>
  </div>
</div>
